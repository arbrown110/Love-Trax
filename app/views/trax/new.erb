<h1>Tell me all about it!!</h1>


<form action="/trax" method= "POST">
    <p>
    <label for= "name">Name:</label>
    <input type="text" name="name" id="name"  />
    </p>
    <p>
    <label for= "date">Date:</label>
    <input type="date" name="date" id="date">
    </p>
    <p>
    <label for= "score">Score:</label>
    <input type="range" name="score" id="score" min="0" max="5" />
    </p>
 
    <p>
    <label for= "location">Location:</label>
    <input type="text" name="location" id="location" />
    </p>
    <p>
    <label for= "phone">Phone Number:</label>
    <input type="tel" name="phone" id="phone" pattern="[0-9]{3}-[0-9]{2}-[0-9]{3}" />
    </p>
    <p>
    <label for= "interest">Interest:</label>
    <input type="radio" name="yes" id="yes" />
    <label for="yes">YES</label>
    <input type="radio" name="no" id="no" />
    <label for="no">NO</label>
    </p>
    <br><br>
    <label for= "user_id">Id:</label>
    <input type="integer" name="user_id" id="user_id"  />


    <p>
    <input type="reset">
    <input type="submit">
    </p>
</form>

